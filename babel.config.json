// vim: ft=json5
{
  "presets": [
    [
      "@babel/preset-env",
      {
        // Uncomment to run Babel in debug mode, which will print out all
        // polyfills and transforms being applied.
        // "debug": true,
        "useBuiltIns": "usage",
        "corejs": "3.16"
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "babel-plugin-styled-components",
    "@babel/plugin-proposal-class-properties",
  ]
}
